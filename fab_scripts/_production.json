{
    "python": "python2.7",
    "url": "http://codeart.herokuapp.com",
    "host": "codeart.herokuapp.com",
    "port": 80,
    "heroku_app": "codeart",
    "heroku_app_addons": ["newrelic", "papertrail", "redistogo", "mongolab"],
    "heroku_worker": null,
    "heroku_worker_addons": ["papertrail", "scheduler", "sendgrid", "mailgun", "mandrill"],
    "heroku_cedar": "cedar-14",
    "paths": [],
    "vars": {
        "DEBUG": "False",
        "HEROKU": "True",
        "AWS_STORAGE_BUCKET_NAME": "$AWS_STORAGE_BUCKET_NAME",
        "AWS_ACCESS_KEY_ID": "$AWS_ACCESS_KEY_ID",
        "AWS_SECRET_ACCESS_KEY": "$AWS_SECRET_ACCESS_KEY",
        "AWS_REGION": "$AWS_REGION"
    },
    "shared_vars": ["REDISTOGO_URL", "REDIS_URL", "MONGOHQ_URL", "MONGOLAB_URI", "DATABASE_URL"]
}
